package coffeProject;


import java.util.GregorianCalendar;

import coffeProject.abstracts.BaseCustomerManager;
import coffeProject.adapter.MernisServiceAdapter;

import coffeProject.concretes.StarbucksCustomerManager;
import coffeProject.entities.Customer;

public class Main {

	public static void main(String[] args) {
		Customer customer = new Customer();
		customer.setId(1);
		customer.setFirstName("Fehmi");
		customer.setLastName("Çitiloğlu");
		customer.setBirthday(new GregorianCalendar(1995, 10 , 8).getTime());
		customer.setNationalId("19937541920");
		
		
		BaseCustomerManager customerManager = new StarbucksCustomerManager();
		customerManager.save(customer);

		
		
	}

}
